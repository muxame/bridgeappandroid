<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="@dimen/_5sdp"></corners>
            <stroke android:color="@color/green_terms_condition" android:width="1dip"/>
            <solid android:color="@color/green_terms_condition"></solid>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/_5sdp"></corners>
            <stroke android:width="1dip" android:color="@color/gray1" />
            <solid android:color="@color/gray1"></solid>
        </shape>
    </item>
</selector>